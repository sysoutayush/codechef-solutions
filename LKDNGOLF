#include<iostream>

using namespace std;

int main()
{   
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n,x,k;
        cin>>n>>x>>k;
        n+=1;
        if((x%k)==0)
        {
            cout<<"YES"<<"\n";
        }
        else if ((n-x)%k==0)
        {
            cout<<"YES"<<"\n";
        }
        else
        {
            cout<<"NO"<<"\n";
        }
    }
}
